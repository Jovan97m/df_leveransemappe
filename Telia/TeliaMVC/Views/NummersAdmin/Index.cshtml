                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.KostnadsstedSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
