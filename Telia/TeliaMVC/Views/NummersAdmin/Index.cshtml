                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("UpdateColumn", "NummersAdmin",new { sortOrder = ViewBag.CurrentSort, currentFilter = ViewBag.CurrentFilter , CopyColumn="date" , currentSelected =ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-arrow-down"></span>
