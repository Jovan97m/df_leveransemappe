@model PagedList.IPagedList<TeliaMVC.Models.Nummer>
@using PagedList.Mvc;
<link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />

@{
    ViewBag.Title = "Brukerliste";
    if (Session["Id"] == null)
    {
        Response.Redirect("~/Login/Index");
    }

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <style>
        .Table {
            box-shadow: 2px 2px 1px 3px lightgray;
            border-radius: 14px;
        }

        .button {
            border-radius: 6px;
            margin: 1px;
        }

        .NotNeeded {
            background-color: #F9B3A7;
        }

        tbody {
            background-color: RGB(230,231,231);
        }

        .inputRadio {
            border-radius: 10px;
            box-shadow: inset 0 1px 1px hsla(0,0%,100%,.8), 0 0 0 1px hsla(0,0%,0%,.6), 0 2px 3px hsla(0,0%,0%,.6), 0 4px 3px hsla(0,0%,0%,.4), 0 6px 6px hsla(0,0%,0%,.2), 0 10px 6px hsla(0,0%,0%,.2);
            cursor: pointer;
            display: inline-block;
            height: 15px;
            margin-right: 16px;
            position: relative;
            width: 16px;
            -webkit-appearance: none;
        }

            .inputRadio:hover {
                background-color: #c86cf9;
            }

            .inputRadio:checked {
                background-color: #990ae4;
            }

        .SelectBox {
            color: black;
            font-size: medium;
            font-weight: bold;
            margin-right: 2px;
            margin-bottom: 4px;
            border-radius: 16px;
            outline-offset: 4px;
            border: 1px solid transparent;
            cursor: pointer;
            box-shadow: 0 8px 14px 0 rgba(0,0,0,0.24), 0 13px 30px 0 rgba(0,0,0,0.19);
        }

        .HeaderTable {
            background-color: #990AE4;
            color: white;
            text-shadow: 1px 2px black;
        }

        .ButtonCreate {
            background-color: #990AE4;
            font-size: medium;
            margin-bottom: 3px;
            font-weight: bold;
            color: white;
            padding: 5px 10px;
            border: solid 1px black;
            border-radius: 16px;
            border-color: transparent;
            text-shadow: 1px 1px black;
            box-shadow: 1px 8px 15px 0 rgba(0,0,0,0.24), 0 13px 30px 0 rgba(0,0,0,0.19);
        }

            .ButtonCreate:hover {
                color: white;
                background-color: purple;
                box-shadow: 0 13px 18px 0 #990AE4, 0 16px 45px 0 rgba(0,0,0,0.21);
            }

            .ButtonCreate:active {
                transform: translateY(+2px);
                background-color: darkblue;
            }

        .ButtonSearch {
            background-color: dodgerblue;
            font-size: medium;
            font-weight: bold;
            color: black;
            padding: 7px 12px;
            border: solid 1px black;
            border-radius: 16px;
            border-color: transparent;
            box-shadow: 1px 8px 15px 0 rgba(0,0,0,0.24), 0 13px 24px 0 rgba(0,0,0,0.19);
        }

            .ButtonSearch:hover {
                color: white;
                background-color: blue;
                box-shadow: 0 13px 18px 0 dodgerblue, 0 16px 45px 0 rgba(0,0,0,0.21);
            }

            .ButtonSearch:active {
                transform: translateY(+2px);
                background-color: dodgerblue;
            }

        .ButtonBack {
            background-color: darkred;
            font-size: medium;
            font-weight: bold;
            color: white;
            padding: 7px 10px;
            border-radius: 16px;
            border-color: transparent;
            text-shadow: 1px 1px black;
            box-shadow: 0 8px 14px 0 rgba(0,0,0,0.24), 0 13px 30px 0 rgba(0,0,0,0.19);
        }

            .ButtonBack:hover {
                color: white;
                background-color: red;
                box-shadow: 0 13px 18px 0 rgb(200, 0, 0), 0 16px 45px 0 rgba(0,0,0,0.19);
            }

            .ButtonBack:active {
                transform: translateY(+2px);
                background-color: darkred;
            }

        .buttonInfoColor {
            width: 15px;
            height: 15px;
            margin-right: 10px;
            cursor: default;
        }

            .buttonInfoColor:hover {
                cursor: default;
            }
    </style>

    <div style="display=inline-block;overflow-x:auto; margin-left:10px; margin-right:10px;" class="shadow">
        <div class="col-sm-4" style="border-right: solid 1px azure;">
            @using (Html.BeginForm("Index", "NummersAdmin", FormMethod.Get))
            {
                <fieldset>
                    <legend>Søkealternativer</legend>
                    <label class="radio-inline">
                        @Html.RadioButton("SearchParameter", "Telefonnummer", htmlAttributes: new { @class = "inputRadio" })<label>Telefonnummer</label>
                    </label>
                    <br />
                    <label class="radio-inline">
                        @Html.RadioButton("SearchParameter", "Fornavn", htmlAttributes: new { @class = "inputRadio" })<label>Fornavn</label>
                    </label>
                    <br />
                    <label class="radio-inline">
                        @Html.RadioButton("SearchParameter", "Binding", htmlAttributes: new { @class = "inputRadio" })<label>Binding</label>
                    </label>
                    <br />
                </fieldset>
                <p style="font-size: medium;font-weight:bold;color:black;margin-left:15px;">
                    Søk etter @Html.TextBox("SearchString", ViewBag.CurrentFilter as string)
                    <input type="submit" value="Search" class="ButtonSearch" />
                </p>
                <div>
                    <fieldset>
                        <legend>
                            Velg firmaet du vil vise data for
                        </legend>
                        <p>
                            @Html.DropDownList("selected", new SelectList(ViewBag.nummers), new { @class = "form-control SelectBox" })
                            <input type="submit" value="Load" class="ButtonCreate shadow-lg" />
                        </p>
                    </fieldset>
                </div>
            }
        </div>
        <div class="col-sm-3" style="margin-bottom: 20px;">
            @using (Html.BeginForm("Creating", "NummersAdmin", FormMethod.Get))
            {
                <fieldset>
                    <legend>
                        Del der du slår nummeret på klienten du vil tilordne den valgte typen nummer til
                    </legend>
                    <p>
                        @Html.DropDownList("selected", new SelectList(ViewBag.nummerAdd), new { @class = "form-control SelectBox" })
                    </p>
                    <label class="radio-inline">
                        @Html.RadioButton("Type", "M", htmlAttributes: new { @class = "inputRadio" })<label>Telefonnummer</label>
                    </label>
                    <br />
                    <label class="radio-inline">
                        @Html.RadioButton("Type", "F", htmlAttributes: new { @class = "inputRadio" })<label>Fixed</label>
                    </label>
                    <br />
                    <label class="radio-inline">
                        @Html.RadioButton("Type", "I", htmlAttributes: new { @class = "inputRadio" })<label>Internet</label>
                    </label>
                    <br />
                    <input type="submit" value="Skriv inn et tall" class="ButtonCreate" />
                </fieldset>
            }
        </div>
        <div class="col-sm-4">
            <fieldset>
                <legend>
                    Instruksjoner for bruk av Excell-filer
                </legend>
                <div class="col-md-6 " style="margin-bottom: 5px;">
                    <p>Opprett fil som data skal lastes inn i</p>
                    <button class="ButtonCreate shadow-lg" data-toggle="tooltip" data-placement="bottom"
                            title="Denne knappen oppretter Excell-fil på datamaskinen, slik at du kan fylle den med data"
                            onclick="location.href = '@Url.Action("Export", "Nummers")'">
                        <span class="glyphicon glyphicon-download"></span> Create Excell
                    </button>
                </div>
                <div class="col-md-5 " style="margin-bottom: 5px;">
                    <p>Last inn dataene fra Excel-filen</p>
                    @using (Html.BeginForm("Excel", "NummersAdmin", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {
                        <div>
                            @Html.DropDownList("id_sesije", new SelectList(ViewBag.nummerAdd), new { @class = "form-control SelectBox" })
                        </div>
                        @Html.Raw(ViewBag.Error)
                        <input type="file" name="excelfile" class="ButtonCreate shadow-lg" />
                        <button class="ButtonCreate shadow-lg" title="Denne knappen oppretter Excell-fil på datamaskinen, slik at du kan fylle den med data"
                                onclick="location.href = '@Url.Action("Excel", "NummersAdmin",new { id_sesija = ViewBag.ID })'">
                            <span class="glyphicon glyphicon-import"></span>Importer excel-fil
                        </button>
                    }
                </div>
            </fieldset>
        </div>
    </div>
   
        <div style="overflow-x:auto; margin-left: 11px; margin-right: 12px;border-radius:12px;" class=".scrollbar-lady-lips">
            <table class="table-hover table-condensed Table" style="font-size: 13px;font-weight:bold;table-layout:fixed;">
                <thead class="HeaderTable">
                    <tr>

                        <th style="width:25px">     E / D       </th>
                        <th class="col-md-6">
                            Telefonnummer
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.Telefonnummer, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort"></span>
                            </button>
                        </th>

                        <th class="col-sm-2">
                            Fornavn
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.FornavnSortParm, currentFilter = ViewBag.CurrentFilter ,currentSelected = ViewBag.CurrentSelected})'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" type="button" data-toggle="modal" data-target="#myModal">
                                <span class="glyphicon glyphicon-arrow-down"></span>
                            </button>
                            <div class="modal fade" id="myModal">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <a href="#" class="close" data-dismiss="modal">&times;</a>
                                            <h3 style="color: red;">Er du sikker på at du vil kopiere til alle medlemmer?</h3>
                                        </div>
                                        @ViewBag.Value=@Url.Action("UpdateColumn", "NummersAdmin", new { sortOrder = ViewBag.CurrentSort, currentFilter = ViewBag.CurrentFilter, CopyColumn = "Fornavn", currentSelected = ViewBag.CurrentSelected })
                                        <p>ViewBag.Value</p>
                                        <div class="modal-body">
                                            <p>Verdi som skal kopieres i alle merkede rader: <span style=" text-decoration: underline; font-weight: bold;"></span></p>
                                        </div>
                                        <div class="modal-footer">
                                            <a href="#" class="btn btn-default" data-dismiss="modal">Tilbake</a>
                                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("UpdateColumn", "NummersAdmin",new { sortOrder = ViewBag.CurrentSort, currentFilter = ViewBag.CurrentFilter , CopyColumn="Etternavn" , currentSelected =ViewBag.CurrentSelected })'">
                                                <span class="glyphicon glyphicon-arrow-down"></span>
                                                </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </th>
                        <th class="col-sm-2">
                            Etternavn
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.EtternavnSortParm, currentFilter = ViewBag.CurrentFilter,currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("UpdateColumn", "NummersAdmin",new { sortOrder = ViewBag.CurrentSort, currentFilter = ViewBag.CurrentFilter , CopyColumn="Etternavn" , currentSelected =ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-arrow-down"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
                            HusNummer
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.Hus_nummerSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("UpdateColumn", "NummersAdmin",new { sortOrder = ViewBag.CurrentSort, currentFilter = ViewBag.CurrentFilter , CopyColumn="Hus_nummer" , currentSelected =ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-arrow-down"></span>
