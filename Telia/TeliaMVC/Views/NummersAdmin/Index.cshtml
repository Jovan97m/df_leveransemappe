                        </th>
                        <th class="col-sm-2">
                            HusBokstav
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.Hus_bokstavSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("UpdateColumn", "NummersAdmin",new { sortOrder = ViewBag.CurrentSort, currentFilter = ViewBag.CurrentFilter , CopyColumn="Hus_bokstav" , currentSelected =ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-arrow-down"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
                            Postnummer
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.PostnummerSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("UpdateColumn", "NummersAdmin",new { sortOrder = ViewBag.CurrentSort, currentFilter = ViewBag.CurrentFilter , CopyColumn="post_nr_" , currentSelected =ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-arrow-down"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
                            Katalogoppføring
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.KatalogoSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
