                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.Abonnementstype, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
                            Binding
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.BindingSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
                            Antall TrillingSIM
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.AntallSIMSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
                            allDataSIM
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.allDataSIMSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
                            Manuell Top-up
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.ManuelSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
                            Sperre Top-up
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.SperreSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
                            Norden
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.NordenSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
                            Tale og SMS til EU
                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("Index", "NummersAdmin",new { sortOrder = ViewBag.TaleSortParm, currentFilter = ViewBag.CurrentFilter, currentSelected = ViewBag.CurrentSelected })'">
                                <span class="glyphicon glyphicon-sort-by-alphabet"></span>
                            </button>
                        </th>
                        <th class="col-sm-2">
