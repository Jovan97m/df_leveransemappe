                            <button class="btn btn-sm" style="background-color:transparent; padding:4px;" onclick="location.href = '@Url.Action("UpdateColumn", "NummersAdmin",new { sortOrder = ViewBag.CurrentSort, currentFilter = ViewBag.CurrentFilter , CopyColumn="Katalop" , currentSelected =ViewBag.CurrentSelected })'">
